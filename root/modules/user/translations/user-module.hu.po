#: modules/user/user.module:242
msgid "You must enter a username."
msgstr "Meg kell adni egy felhasználónevet."

#: modules/user/user.module:243
msgid "The username cannot begin with a space."
msgstr "A felhasználói név nem kezdődhet szóközzel."

#: modules/user/user.module:244
msgid "The username cannot end with a space."
msgstr "A felhasználói név nem végződhet szóközzel."

#: modules/user/user.module:245
msgid "The username cannot contain multiple spaces in a row."
msgstr "A felhasználói név nem tartalmazhat több egymás utáni szóközt."

#: modules/user/user.module:246;257
msgid "The username contains an illegal character."
msgstr "A felhasználói név nem engedélyezett karaktert tartalmaz."

#: modules/user/user.module:259
msgid "The username is not a valid authentication ID."
msgstr "A felhasználói név hibás azonosító."

#: modules/user/user.module:260
msgid "The username %name is too long: it must be %max characters or less."
msgstr ""
"A felhasználói név („%name”) túl hosszú, legfeljebb %max karakteres lehet."

#: modules/user/user.module:264
msgid "You must enter an e-mail address."
msgstr "Meg kell adni egy email címet."

#: modules/user/user.module:266
msgid "The e-mail address %mail is not valid."
msgstr "Az email cím („%mail”) hibás."

#: modules/user/user.module:281
msgid "The uploaded file was not an image."
msgstr "A feltöltött fájl nem kép."

#: modules/user/user.module:287
msgid "The uploaded image is too large; the maximum file size is %size kB."
msgstr ""
"A feltöltött kép túl nagy, a legnagyobb engedélyezett fájlméret %size kB."

#: modules/user/user.module:290
msgid ""
"The uploaded image is too large; the maximum dimensions are %dimensions "
"pixels."
msgstr "A feltöltött kép túl nagy, legfeljebb %dimensions képpont lehet."

#: modules/user/user.module:298
msgid ""
"Failed to upload the picture image; the %directory directory doesn't exist."
msgstr "A kép feltöltése sikertelen, „%directory” könyvtár nem létezik."

#: modules/user/user.module:430;724
